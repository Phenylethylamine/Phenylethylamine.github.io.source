<div id="content" class="container my-sm-5 p-0">
    <div class="row m-0">
        <div id="aside" class="col-12 col-md-4 col-lg-3 p-0 order-0 order-md-1">
            <div id="tocSection">
                <div class="title py-4">
                    <i class="fas fa-list"></i>
                    <span>목차</span>
                </div>
                <div class="markdownToc d-none d-sm-block">
                    <%- toc(page.content, {list_number: false}) %>
                </div>
            </div>
        </div>
        <div id="document" class="col-12 col-md-8 col-lg-9 p-0 order-1 order-md-0">
            <div class="markdown p-4 p-sm-5">
                <%- page.content %>
            </div>
        </div>
    </div>
</div>